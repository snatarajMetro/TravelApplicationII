<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <link rel="icon" href="/favicon.ico" />
    <link href="/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/dropzone.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="http://ui-grid.info/release/ui-grid.css" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans+Condensed:300" rel="stylesheet">
    <link href="/Content/Style.css" rel="stylesheet" type="text/css" />

    @*<script src="/Scripts/modernizr-2.8.3.js"></script>*@
    <script src="/Scripts/jquery-3.1.1.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
    <script src="http://ui-grid.info/release/ui-grid.js"></script>

    <script src="/Scripts/travelrequest.js"></script>
    <script src="/Scripts/travelapp.js"></script>
    <script src="/Scripts/dropzone.js"></script>
</head>
<body class="appbackground">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div style="float:left">
            <img src="~/Images/logo.jpg" width="45" height="45" />
        </div>
        <div>
            @Html.ActionLink("Travel Request / Authorization", "Index", "Login", new { area = "" }, new { @class = "logotitle" })
        </div>
        <div id="logout" class="logout">
            <span onclick="logout()">Log out</span>
        </div>
    </div>
    <div class="container body-content">
        <div class="maincontent">
        @RenderBody()
        </div>
    </div>
    @*<div class="copyright">
        <p>Copyright &copy; @DateTime.Now.Year Metro</p>
    </div>*@
</body>
</html>
